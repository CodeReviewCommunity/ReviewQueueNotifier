{
  "manifest_version": 2,

  "name": "Review Queue Notifier",
  "description": "Alerts you to new reviews on your favorite Stack Exchange site",
  "version": "0.1",
  "icons": "https://github.com/malachi26/ReviewQueueNotifier/blob/ChromeExtension/Resources/Icon2.jpg",

  "page_action": {
    "default_icon": "https://github.com/malachi26/ReviewQueueNotifier/blob/ChromeExtension/Resources/Icon2.jpg",
    "default_popup": "controlPanel.html",
	"default_title": "Review Queue Notifier"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/",
	"Notifications",
	"contextMenus"
  ],
  "content_scripts": [
	{
		"matches": ["http://codereview.stackexchange.com/review"],
		"js": ["script1.js"],
		"run_at": "document_end"
	}
  ],
  "background_page": "background.html"
}